<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
     <!-- bootstrap css  4-->
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha1/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-GLhlTQ8iRABdZLl6O3oVMWSktQOp6b7In1Zl3/Jr59b6EGGoI1aFkw7cmDA6j6gD" crossorigin="anonymous">
    <link rel="stylesheet" href="Maincss/usersign-in.css">
    
    <!-- bootstarp js -->
    <script src="https://cdn.jsdelivr.net/npm/jquery@3.5.1/dist/jquery.slim.min.js" integrity="sha384-DfXdz2htPH0lsSSs5nCTpuj/zy4C+OGpamoFVy38MVBnE+IbbVYUew+OrCXaRkfj" crossorigin="anonymous"></script>
<script src="https://cdn.jsdelivr.net/npm/bootstrap@4.6.2/dist/js/bootstrap.bundle.min.js" integrity="sha384-Fy6S3B9q64WdZWQUiU+q4/2Lc9npb8tCaSX9FK7E8HnRr0Jz8D6OP9dO5Vg3Q9ct" crossorigin="anonymous"></script>
    
    <title>Document</title>
</head>
<body>
    <div class="body1">
        <img class="img1" src="Maincss/images/signin (2).jpeg" alt="">

    <div class="box">

    </div>
    <div class="card">
        <div class="user-signup " >
            <h1 class="user">Owner registration</h1>
            <form action="/ownersignup" class="form1" method="post">
                <table>
                    <tr>
                        <td>
                            <div >               
                                <input type="text" class="form-group"  name="name" placeholder="Name" required>
                            </div>
                            <br>
                        </td>
                    </tr>
                    <tr>
                        <td>
                            <div >               
                                <input type="text" class="form-group"  name="mobile" placeholder="Mobile" aria-required="true">
                            </div>
                            <br>

                        </td>
                    </tr>
                    <tr>
                        <td>                
                            <div>             
                            <input type="email" class="form-group" id="inputEmail" name="Mail_id" placeholder="Email" required>
                            </div>
                            <br>
                        </td>
                    </tr>
                    <tr>
                        <td>                
                            <div>             
                            <input type="text" class="form-group" name="aadhar" placeholder="Aadhar" required>
                            </div>
                            <br>
                        </td>
                    </tr>
                    
                    <tr>
                        <td>                
                            <div>             
                            <input type="text" class="form-group" name="Station-Name" placeholder="Station Name" required>
                            </div>
                            <br>
                        </td>
                    </tr>

                    
                    <tr>
                        <td>                
                            <div>  
                                <label for="level-1">Level 1</label>           
                                <input type="text" class="form-group" name="Level1" id="level-1" placeholder="Is-Available" required>
                                <input type="text" name="Slots1" placeholder="Enter First slot">
                                <input type="text" name="count1" placeholder="Enter Number of slots">
                            </div>
                            <br>
                        </td>
                    </tr>
                    <tr>
                        <td>                
                            <div>  
                                <label for="level-2">Level 2</label>           
                                <input type="text" class="form-group" name="Level2" id="level-2" placeholder="Is-Available" required>
                                <input type="text" name="Slots2" placeholder="Enter First slot">
                                <input type="text" name="count2" placeholder="Enter Number of slots">
                            </div>
                            <br>
                        </td>
                    </tr>
                    <tr>
                        <td>                
                            <div>  
                                <label for="level-3">Level 3</label>           
                                <input type="text" class="form-group" name="Level3" id="level-3" placeholder="Is-Available" required>
                                <input type="text" name="Slots3" placeholder="Enter first slot">
                                <input type="text" name="count3" placeholder="Enter Number of slots">
                            </div>
                            <br>
                        </td>
                    </tr>

                    <tr>
                        <td>
                            <div >               
                                <input type="text" class="form-group" data-private="lipsum"  id="colFormLabel" name="userName" placeholder="User name "  required>
                            </div>
                            <br>
                        </td>
                    </tr>
                    <tr>
                        <td>
                            <div >               
                                <input type="password" class="form-group"  id="password" name="password" placeholder="Password" required>
                            </div>
                            <br>
                        </td>                       
                    </tr>
                </table>
                <button type="submit" class="btn btn-info">Continue</button>
                <br>
                <p class="text1">already had an account! |<a href="/login">login</a></p>
            </form>

            <!-- <p class="text1">NOTE: please make sure correct passowrd</p> -->
    
        </div>
        
    </div>
    </div>

    <!-- <div class="red">
        red
    </div> -->
</body>
</html>