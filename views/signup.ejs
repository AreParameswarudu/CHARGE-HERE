    <div class="box">

    </div>
    <div class="card">
        <div class="user-signup " >
            <h1 class="user">User sign up</h1>
            <form action="signup.html" class="form1" method="post">
                <table>
                    <tr>
                        <td>
                            <div >               
                                <input type="text" class="form-group" data-private="lipsum"  id="colFormLabel" placeholder="User name "  required>
                            </div>
                            <br>
                        </td>
                    </tr>
                    <tr>
                        <td>                
                            <div >             
                            <input type="email" class="form-group" id="inputEmail" placeholder="Email" required>
                            </div>
                            <br>
                        </td>
                    </tr>
                    <tr>
                        <td>
                            <div >               
                                <input type="password" class="form-group"  id="password" placeholder="Password" required>
                            </div>
                            <br>
                        </td>
                        
                    </tr>
                    <tr>
                        <td>
                            <div >               
                                <input type="email" class="form-group"  id="password" placeholder="Postal-code" required>
                            </div>
                            <br>
                        </td>
                    </tr>
                    <tr>
                        <td>
                            <div >               
                                <input type="location" class="form-group"  id="" placeholder="Location" aria-required="true">
                            </div>
                            <br>

                        </td>
                    </tr>
                </table>
                <button type="submit" class="btn btn-info">Next</button>
                <br>
                <p class="text1">already had an account! |<a href="/login">login</a></p>
            </form>

